{
  "consumer": {
    "name": "Budget Client"
  },
  "interactions": [
    {
      "description": "a request to add an envelope with the object pattern",
      "providerState": "i have a list of envelopes",
      "request": {
        "body": {
          "budget": 100,
          "name": "Groceries"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "method": "POST",
        "path": "/envelopes"
      },
      "response": {
        "body": {
          "budget": 100,
          "id": 1,
          "name": "Groceries"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "$.body.id": {
            "match": "type"
          }
        },
        "status": 201
      }
    }
  ],
  "metadata": {
    "pact-js": {
      "version": "15.0.1"
    },
    "pactRust": {
      "ffi": "0.4.22",
      "models": "1.2.3"
    },
    "pactSpecification": {
      "version": "2.0.0"
    }
  },
  "provider": {
    "name": "Budget API"
  }
}